  "scripts": {
    "build": "rimraf dist && npm cache verify && tsc && npm run doc",
    "build-sources": "tsc",
    "prepublishOnly": "npm run build",
    "doc": "rimraf docs && ./node_modules/.bin/compodoc ./src -p tsconfig.json -d ./docs --theme gitbook --hideGenerator --disableCoverage --toggleMenuItems default --silent && cpx AHOI-Logo.png ./docs/AHOI-Logo.png",
    "uml": "./node_modules/.bin/tsviz -r ./src/ ./docs/uml.png",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "ahoi-swagger-fetchclient": "https://github.com/sparkassen-hub/ahoi-swagger-fetchclient",
    "cpx": "^1.5.0",
    "jsonwebtoken": "^8.3.0",
    "node-fetch": "^2.2.0"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.6",
    "@types/crypto-js": "^3.1.41",
    "@types/jsonwebtoken": "^7.2.8",
    "@types/node-fetch": "^2.1.2",
    "rimraf": "^2.6.2",
    "tslint": "5.11.0",
    "tslint-config-airbnb": "^5.11.0",
    "tsviz": "^1.0.11",
    "typedoc": "0.13.0",
    "typescript": "3.1.5"
  },
  "engines": {
    "node": ">=10.0.0"
  }
